{
  "name": "chattoo-client",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "apollo:generate": "npx apollo codegen:generate --target typescript --endpoint=http://localhost:5000/graphql --outputFlat --exludes=node_modules --queries **/*.tsx src/generated",
    "apollo:generate-old": "apollo codegen:generate --target typescript --excludes=node_modules/* --queries **/*.tsx --endpoint http://localhost:5000/graphql --tagName=gql --outputFlat=./src/common/interfaces/schema-types.ts",
    "graphql-generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "g": "^2.0.1",
    "graphql": "^15.0.1",
    "immer": "^9.0.6",
    "next": "^12.0.3",
    "oidc-client": "^1.11.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "subscriptions-transport-ws": "^0.9.19",
    "typescript-common": "^1.0.5-beta",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-styled-components": "^1.12.0",
    "graphql-codegen-typescript-common": "^0.18.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.1.5"
  }
}
